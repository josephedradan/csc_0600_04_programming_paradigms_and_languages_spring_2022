{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": ["es2015", "dom", "es2016", "es6"],
    "experimentalDecorators": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "moduleResolution": "Node",
    "alwaysStrict": true,
    "strict": true
  }
}
